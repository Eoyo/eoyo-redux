{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "dist": ["dist"]
    },
    "allowJs": true,
    "checkJs": true,
    "downlevelIteration": true
  },
  "include": ["src", "__test__"],
  "exclude": ["node_modules"]
}
